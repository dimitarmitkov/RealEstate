<h1>Saerch Properties By Year</h1>
<form method="get" action="/Properties/DoSearchYear" class="col-md-6">
    <div class="form-control">
        Min Year: <input name="minYear" />
    </div>
    <hr />
    <div class="form-control">
        Max Year: <input name="maxYear" />
    </div>
    <hr />
    <div class="form-control">
        Min Size: <input name="minSize" />
    </div>
    <hr />
    <div class="form-control">
        Max Size: <input name="maxSize" />
    </div>
    <hr />
    <input type="submit" class="btn btn-primary" value="Search by Year/Size" />
</form>
